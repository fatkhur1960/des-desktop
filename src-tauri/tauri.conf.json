{
  "build": {
    "distDir": "../dist/tauri-api-demos",
    "devPath": "http://localhost:4200",
    "beforeDevCommand": "",
    "beforeBuildCommand": ""
  },
  "ctx": {
    "debug": false,
    "exitOnPanic": false
  },
  "tauri": {
    "embeddedServer": {
      "active": false
    },
    "bundle": {
      "active": true,
      "targets": [
        "deb",
        "osx",
        "msi"
      ],
      "identifier": "io.fanonym.des-ui",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "resources": [
        "tokoku.db"
      ],
      "externalBin": [],
      "copyright": "fatkhuranonym@gmail.com",
      "category": "DeveloperTool",
      "shortDescription": "Implementasi double exponential smoothing",
      "longDescription": "Implementasi double exponential smoothing pada penjualan",
      "deb": {
        "depends": [],
        "useBootstrapper": false
      },
      "osx": {
        "frameworks": [],
        "minimumSystemVersion": "",
        "useBootstrapper": false
      },
      "exceptionDomain": ""
    },
    "allowlist": {
      "all": false,
      "answer": true,
      "event": true, 
      "execute": false,
      "listFiles": false,
      "open": false, 
      "readBinaryFile": false,
      "readTextFile": false,
      "setTitle": true, 
      "writeFile": true
    },
    "window": {
      "title": "DES - TokoKu",
      "width": 800,
      "height": 600,
      "resizable": true,
      "fullscreen": true
    },
    "security": {
      "csp": "default-src blob: data: filesystem: ws: http: https: 'unsafe-eval' 'unsafe-inline'"
    },
    "inliner": {
      "active": true
    }
  }
}